    "version": "0.2.0",
    "configurations": [
        {
            "name": "generate",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "generate"
            ],
        },
        {
            "name": "fetch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "fetch",
                "--watch",
            ],
        }
    ]
}